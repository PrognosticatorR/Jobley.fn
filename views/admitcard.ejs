<% include partials/subheader %>

    <div class="container">
        <div class="alert alert-info" role="alert">
            <h3 class="text-center "><strong>View Your <%=header%></strong></h3>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-condensed table-bordered">
                            <tr>
                                <th>Title</th>
                                <th>Date</th>
                                <th>Link For Download/Details</th>
                            </tr>
                            <% admitcards.slice().reverse().forEach(function(card){ %>
                                <tr>
                                    <td><span><%= card.title %></span></td>
                                    <td><span><%= card.created.toDateString() %></span></td>
                                    <td class="jobs_table"><a href="<%=card.download%>"><span>Go To <%=header%> Page</span></a></td>
                                </tr>
                                <% }) %>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <% include partials/subfooter %>